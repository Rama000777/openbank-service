pipeline {
	agent any
	stages {
		stage('Checkout') {
			steps {
			echo 'Checkout'
			}
		}
		stage('Build') {
			steps {
			echo 'Clean Build'
			bat 'mvn clean compile'
			}
		}
		stage('Test') {
			steps {
			echo 'Testing'
			bat 'mvn test'
			}
		}
		stage('JaCoco') {
			steps {
			echo 'Code Coverage'
			jacoco()
			}
		}
		stage('Package') {
			steps {
			echo 'Packaging'
			bat 'mvn package -DskipTests'
		}
	}
}